= stylesheet_link_tag 'admin'

%h1 All Mentees
%table#mentees
  %thead
    %tr
      %th cec_id
      %th User role
      %th Work Location
      %th Delete
  %tbody
  - @mentees.each do |mentee| 
    %tr
      %td= mentee.cec_id
      %td= mentee.user_role
      %td= mentee.work_city
      %td= link_to "delete",mentee, method: :delete,data: { confirm: "You sure?" }

