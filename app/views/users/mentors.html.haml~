%h1 All Mentors
%table#mentors
  %thead
    %tr
      %th Email ID
      %th User role
      %th Work Group
      %th Work Location
      %th Password
      %th Delete
  %tbody
  - @mentors.each do |mentor| 
    %tr
      %td= mentor.cisco_email
      %td= mentor.user_role
      %td= mentor.current_group
      %td= mentor.work_city
      %td= mentor.password_digest    
      %td= link_to "delete",mentor, method: :delete,data: { confirm: "You sure?" }
